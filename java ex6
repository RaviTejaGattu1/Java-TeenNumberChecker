public static boolean isTeen(int age) {
        if (age>12&&age<20){
            return true;
        }
        return false;
    }
    public static boolean hasTeen(int age1, int age2, int age3){
        if((isTeen(age1))||(isTeen(age2))||(isTeen(age3))){
            return true;
        }return false;
    }
